{
  "name": "react-native-chatbot-expo",
  "version": "0.0.1-alpha.12",
  "description": "React Native Chatbot for Expo",
  "main": "lib/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint lib/*.js",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "test": "./node_modules/.bin/mocha --require config/mocha.js --compilers js:@babel/register --recursive tests/**/*.spec.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "nyc npm test"
  },
  "files": [
    "lib"
  ],
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ],
    "extension": [
      ".jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sibelly/react-native-chatbot-expo"
  },
  "keywords": [
    "react",
    "chat",
    "chatbot"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LucasBassetti/react-native-chatbot-expo/issues"
  },
  "homepage": "https://github.com/LucasBassetti/react-native-chatbot-expo#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/register": "^7.16.0",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^4.0.1",
    "chai": "^4.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^1.3.1",
    "jsdom": "^18.0.1",
    "metro-react-native-babel-preset": "^0.66.2",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0",
    "react-addons-test-utils": "^15.5.0",
    "react-native-mock-render": "^0.1.9",
    "sinon": "^7.2.3"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.7.2",
    "random-id": "1.0.3",
    "react-native-loading-dots": "^1.2.0"
  },
  "peerDependencies": {
    "react": ">= 0.14.0 < 17.0.0-0",
    "react-native": ">= 0.45.0",
    "styled-components": ">= 2.0.0 < 4"
  }
}
